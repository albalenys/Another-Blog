<header>
  <%= link_to "Another Blog.", root_path, class: "logo" %>

  <nav>
    <% if logged_in? %>
      <%= link_to "Create Post", new_post_path, class: "button" %> |
      <%= link_to "Profile.", user_path(session[:user_id]), class: "button" %> |
      <a class="button" href="">Log Out</a>
    <% else %>
      <%= link_to "Sign Up", new_user_path, class: "button" %> |
      <a class="button" href="">Log In</a>
    <% end %>
  </nav>
</header>
